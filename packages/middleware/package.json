    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "ts-patch": "^1.2.4",
    "typescript": "^3.9.5",
    "typescript-transform-paths": "^1.1.14"
